- id = 336
- priority = 0
- step_value = 0.1
- factor = 1.0
- update_period = future
- max_value = 3.0
- min_value = 0.8
- start_value = 0.8
- query =

cost_per_old_house = AREA(old_houses_insulation_cost_constant_1) + AREA(old_houses_insulation_cost_constant_2) * USER_INPUT();
total_cost = cost_per_old_house * Q(number_of_old_residences);
saving_fraction = 1.0 - AREA(old_houses_insulation_constant_1) / (USER_INPUT() - AREA(old_houses_insulation_constant_2));

EACH(
  UPDATE(V(households_old_houses_heating_savings_from_insulation), initial_investment, total_cost),
  UPDATE(FIRST(LINKS(INPUT_SLOTS(V(households_old_houses_useful_demand_for_heating),useable_heat))), share, saving_fraction),
  UPDATE(FIRST(LINKS(INPUT_SLOTS(V(households_old_houses_useful_demand_for_cooling),cooling))), share, saving_fraction)
)

