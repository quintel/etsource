- use = energetic
- energy_balance_group = final consumption
- groups = [final_demand_cbs]
- co2_free = 0.0
- demand_expected_value = 153040000000.0

~ demand =
    EB(industry, heat) -
    EB(energy_industry_own_use, heat) +
    EB("own_use_in_electricity,_CHP_and_heat_plants", heat) +
    EB(blast_furnaces_own_use, heat) +
    EB(coke_ovens_own_use, heat)
