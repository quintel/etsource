# If no own use is present, use dummy values for input carriers

- query =
    total_input = DATASET_INPUT(input_energy_blastfurnace_own_use_coal) + DATASET_INPUT(input_energy_blastfurnace_own_use_cokes) + DATASET_INPUT(input_energy_blastfurnace_own_use_coal_gas) + DATASET_INPUT(input_energy_blastfurnace_own_use_electricity) + DATASET_INPUT(input_energy_blastfurnace_own_use_network_gas) + DATASET_INPUT(input_energy_blastfurnace_own_use_steam_hot_water) + DATASET_INPUT(input_energy_blastfurnace_own_use_crude_oil);

    IF(total_input > 0.0,
        {coal: DATASET_INPUT(input_energy_blastfurnace_own_use_coal) / total_input,
        cokes: DATASET_INPUT(input_energy_blastfurnace_own_use_cokes) / total_input,
        coal_gas: DATASET_INPUT(input_energy_blastfurnace_own_use_coal_gas) / total_input,
        electricity: DATASET_INPUT(input_energy_blastfurnace_own_use_electricity) / total_input,
        network_gas: DATASET_INPUT(input_energy_blastfurnace_own_use_network_gas) / total_input,
        steam_hot_water: DATASET_INPUT(input_energy_blastfurnace_own_use_steam_hot_water) / total_input,
        crude_oil: DATASET_INPUT(input_energy_blastfurnace_own_use_crude_oil) / total_input},
        {coal: 0.0,
        cokes: 0.0,
        coal_gas: 0.9612,
        electricity: 0.0388 ,
        network_gas: 0.0,
        steam_hot_water: 0.0,
        crude_oil: 0.0})
