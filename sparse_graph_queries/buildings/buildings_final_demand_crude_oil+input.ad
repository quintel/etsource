# Sets the oil mix for the crude oil node

- query =
  {
    diesel: DATASET_INPUT(input_percentage_of_diesel_buildings_final_demand_crude_oil),
    biodiesel: DATASET_INPUT(input_percentage_of_biodiesel_buildings_final_demand_crude_oil),
    kerosene: DATASET_INPUT(input_percentage_of_kerosene_buildings_final_demand_crude_oil),
    bio_kerosene: DATASET_INPUT(input_percentage_of_bio_kerosene_buildings_final_demand_crude_oil),
    lpg: DATASET_INPUT(input_percentage_of_lpg_buildings_final_demand_crude_oil),
    bio_oil: DATASET_INPUT(input_percentage_of_bio_oil_buildings_final_demand_crude_oil),
    crude_oil: DATASET_INPUT(input_percentage_of_crude_oil_buildings_final_demand_crude_oil)
  }
