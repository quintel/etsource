
- query =
    fertilizers_network_gas_non_energetic_demand = DATASET_INPUT(input_industry_chemical_fertilizers_network_gas_demand);
    refineries_network_gas_non_energetic_demand = DATASET_INPUT(input_industry_chemical_refineries_network_gas_demand);
    chemical_network_gas_non_energetic_demand = fertilizers_network_gas_non_energetic_demand + refineries_network_gas_non_energetic_demand + DATASET_INPUT(input_industry_chemical_other_network_gas_non_energetic_demand);
    other_network_gas_non_energetic_demand = DATASET_INPUT(input_industry_other_network_gas_non_energetic_demand);
    industry_total_network_gas_non_energetic_demand = chemical_network_gas_non_energetic_demand + other_network_gas_non_energetic_demand;
    industry_total_network_gas_non_energetic_demand = IF(industry_total_network_gas_non_energetic_demand > 0.0, industry_total_network_gas_non_energetic_demand, 1.0);

    chemical_network_gas_non_energetic_demand / industry_total_network_gas_non_energetic_demand
