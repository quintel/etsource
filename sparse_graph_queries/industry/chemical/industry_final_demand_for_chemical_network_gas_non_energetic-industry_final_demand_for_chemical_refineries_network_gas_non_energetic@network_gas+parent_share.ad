    chemical_refineries_network_gas_non_energetic_demand =  DATASET_INPUT(input_industry_chemical_refineries_network_gas_non_energetic);
    chemical_network_gas_non_energetic_demand = chemical_fertilizers_network_gas_non_energetic_demand + chemical_refineries_network_gas_non_energetic_demand + DATASET_INPUT(input_industry_chemical_other_network_gas_non_energetic_demand);
    chemical_network_gas_non_energetic_demand = IF(chemical_network_gas_non_energetic_demand > 0.0, chemical_network_gas_non_energetic_demand, 1.0);

    chemical_refineries_network_gas_non_energetic_demand / chemical_network_gas_non_energetic_demand
