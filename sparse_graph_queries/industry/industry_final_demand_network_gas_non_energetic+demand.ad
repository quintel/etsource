# final demand equals user inputs of all subsector demands

- query =
    fertilizers_network_gas_non_energetic_demand = DATASET_INPUT(input_industry_chemical_fertilizers_network_gas_non_energetic_demand);
    refineries_network_gas_non_energetic_demand = DATASET_INPUT(input_industry_chemical_refineries_network_gas_non_energetic_demand);
    chemical_network_gas_non_energetic_demand = fertilizers_network_gas_non_energetic_demand + refineries_network_gas_non_energetic_demand + DATASET_INPUT(input_industry_chemical_other_network_gas_non_energetic_demand);
    other_network_gas_non_energetic_demand = DATASET_INPUT(input_industry_other_network_gas_non_energetic_demand);

    chemical_network_gas_non_energetic_demand + other_network_gas_non_energetic_demand
