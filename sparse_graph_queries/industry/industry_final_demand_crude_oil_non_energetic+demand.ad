# final non_energetic_demand equals user inputs of all subsector non_energetic_demands

- query =
    fertilizers_crude_oil_non_energetic_demand = DATASET_INPUT(input_industry_chemical_fertilizers_crude_oil_non_energetic_demand);
    chemical_crude_oil_non_energetic_demand = fertilizers_crude_oil_non_energetic_demand + DATASET_INPUT(input_industry_chemical_other_crude_oil_non_energetic_demand);
    other_crude_oil_non_energetic_demand =   DATASET_INPUT(input_industry_other_crude_oil_non_energetic_demand);

     chemical_crude_oil_non_energetic_demand + other_crude_oil_non_energetic_demand
