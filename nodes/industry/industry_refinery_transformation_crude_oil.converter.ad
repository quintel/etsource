- use = energetic
- energy_balance_group = refinery
- groups = [chemical_industry]
- free_co2_factor = 0.0

~ input.useable_heat = EFFICIENCY(industry_refinery_transformation_crude_oil, input, useable_heat)
~ input.crude_oil = EFFICIENCY(industry_refinery_transformation_crude_oil, input, crude_oil)
~ output.refinery_gas = EFFICIENCY(industry_refinery_transformation_crude_oil, output, refinery_gas)
~ output.lpg = EFFICIENCY(industry_refinery_transformation_crude_oil, output, lpg)
~ output.gasoline = EFFICIENCY(industry_refinery_transformation_crude_oil, output, gasoline)
~ output.kerosene = EFFICIENCY(industry_refinery_transformation_crude_oil, output, kerosene)
~ output.diesel = EFFICIENCY(industry_refinery_transformation_crude_oil, output, diesel)
~ output.heavy_fuel_oil = EFFICIENCY(industry_refinery_transformation_crude_oil, output, heavy_fuel_oil)
~ output.oil_products = EFFICIENCY(industry_refinery_transformation_crude_oil, output, oil_products)
- output.loss = elastic