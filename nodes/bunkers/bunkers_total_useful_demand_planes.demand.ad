- use = energetic
- scaling_exempt = true
- energy_balance_group = []
- groups = [preset_demand]
- graph_methods = [demand]
- free_co2_factor = 0.0

~ demand =
    -EB(international_aviation_bunkers, kerosene_type_jet_fuel) +
      -EB(international_aviation_bunkers, other_kerosene) +
        -EB(international_aviation_bunkers, other_liquid_biofuels)
