- query =
      UPDATE(
             V(industry_final_demand_for_other_dsr_load_shifting_electricity),
             typical_input_capacity, 
             PRODUCT(
                     (USER_INPUT() / 100.0),
                     V(industry_final_demand_for_other_dsr_load_shifting_electricity,electricity_output_capacity)
            )
      )


- priority = 2
- max_value = 50.0
- min_value = 0.0
- start_value = 0.0
- step_value = 1.0
- unit = %
- update_period = future
