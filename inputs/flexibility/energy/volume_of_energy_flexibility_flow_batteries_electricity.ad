- query =
    UPDATE(
      V(energy_flexibility_flow_batteries_electricity, storage),
      "volume",
      PRODUCT(
        DIVIDE(USER_INPUT(), 100),
        DIVIDE(
          INPUT_VALUE(capacity_of_energy_flexibility_flow_batteries_electricity),
          V(energy_flexibility_flow_batteries_electricity,number_of_units)
        )
      )
    )
- priority = 0
- max_value = 500.0
- min_value = 0.0
- start_value_gql = present:PRODUCT(DIVIDE(V(energy_flexibility_flow_batteries_electricity, "storage.volume"),V(energy_flexibility_flow_batteries_electricity,typical_input_capacity)),100)
- step_value = 0.1
- unit = %
- update_period = future
