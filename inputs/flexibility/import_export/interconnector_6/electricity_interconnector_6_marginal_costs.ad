- query =
    UPDATE(
      V(
        energy_interconnector_6_imported_electricity,
        energy_interconnector_6_exported_electricity
      ),
      marginal_costs,
      USER_INPUT()
    )
- priority = 0
- max_value = 500
- min_value = 0
- start_value_gql = present:Q(price_of_imported_electricity)
- step_value = 0.1
- unit = euro
- update_period = future
