- query =
    UPDATE(
        V(bunkers_flexibility_p2g_electricity),
        number_of_units,
        USER_INPUT() / V(bunkers_flexibility_p2g_electricity,typical_input_capacity)
    )
- priority = 0
- max_value_gql = present:MAX(1.0, PRODUCT(DIVIDE(Q(total_electricity_consumed), SECS_PER_YEAR), 16))
- min_value = 0.0
- start_value = 0.0
- step_value = 1.0
- unit = MW
- update_period = future
