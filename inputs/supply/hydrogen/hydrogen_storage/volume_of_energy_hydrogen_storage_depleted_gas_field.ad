- query =
    UPDATE(
      V(energy_hydrogen_storage_depleted_gas_field, storage),
      "volume",
      PRODUCT(
        USER_INPUT(),
        V(energy_hydrogen_storage_depleted_gas_field,typical_input_capacity)
      )
    )
- priority = 0
- max_value = 100.0
- min_value = 0.5
- start_value_gql = present:DIVIDE(V(energy_hydrogen_storage_depleted_gas_field, "storage.volume"),V(energy_hydrogen_storage_depleted_gas_field,typical_input_capacity))
- step_value = 0.1
- unit = hours
- update_period = future
