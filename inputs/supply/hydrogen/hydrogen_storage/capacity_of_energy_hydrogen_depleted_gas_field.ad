      number_of_units,
      USER_INPUT() / V(energy_hydrogen_storage_depleted_gas_field, typical_input_capacity)
    )
- priority = 0
- max_value_gql = present:MAX(1.0, PRODUCT(DIVIDE(Q(total_gas_consumed), SECS_PER_YEAR), 6))
