- query =
    EACH(
        UPDATE(
            V(energy_hydrogen_biomass_gasification),
            number_of_units,
            (1.0 - DIVIDE(INPUT_VALUE(share_of_energy_hydrogen_biomass_gasification_ccs),100.0)) * USER_INPUT() / V(energy_hydrogen_biomass_gasification, typical_input_capacity)
        ),
        UPDATE(
            L(energy_hydrogen_biomass_gasification),
            preset_demand_by_hydrogen_production,
            V(energy_hydrogen_biomass_gasification, hydrogen_production_based_on_number_of_units)
        ),
        UPDATE(
            V(energy_hydrogen_biomass_gasification_ccs),
            number_of_units,
            DIVIDE(INPUT_VALUE(share_of_energy_hydrogen_biomass_gasification_ccs),100.0) * USER_INPUT() / V(energy_hydrogen_biomass_gasification_ccs, typical_input_capacity)
        ),
        UPDATE(
            L(energy_hydrogen_biomass_gasification_ccs),
            preset_demand_by_hydrogen_production,
            V(energy_hydrogen_biomass_gasification_ccs, hydrogen_production_based_on_number_of_units)
        )
    )
- priority = 0
- max_value_gql = present:MAX(500,DIVIDE(Q(total_gas_consumed),PRODUCT(V(energy_hydrogen_biomass_gasification,full_load_hours),MJ_PER_MWH)))
- min_value = 0.0
- start_value_gql = present:SUM(V(energy_hydrogen_biomass_gasification,"number_of_units*typical_input_capacity"),V(energy_hydrogen_biomass_gasification_ccs,"number_of_units*typical_input_capacity"))
- unit = MW
- update_period = future
