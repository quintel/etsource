- query =
    UPDATE(
      EDGE(
        energy_distribution_ht_imported_heat,
        energy_import_heat
      ),
      co2_per_mj,
      DIVIDE(USER_INPUT(), THOUSANDS)
    )
- priority = 0
- max_value = 100.0
- min_value = 0.0
- start_value_gql = present:PRODUCT(V(CARRIER(imported_heat), co2_conversion_per_mj), THOUSANDS)
- step_value = 0.1
- unit = kg/GJ
- update_period = future
