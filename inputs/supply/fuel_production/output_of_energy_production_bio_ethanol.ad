- query =
    UPDATE(
        V(energy_production_bio_ethanol),
        demand,
        DIVIDE((USER_INPUT() * BILLIONS), V(energy_production_bio_ethanol, bio_ethanol_output_conversion))
    )
- priority = 0
- max_value_gql = present:PRODUCT(SUM(Q(final_demand_of_biomass_products),Q(final_demand_of_oil_and_derivatives)),3)
- min_value = 0.0
- start_value_gql = present:Q(output_of_bio_ethanol)
- step_value = 0.1
- unit = PJ
- update_period = future
