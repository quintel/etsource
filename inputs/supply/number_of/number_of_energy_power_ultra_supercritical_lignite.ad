- query =
    EACH(
      UPDATE(V(energy_power_ultra_supercritical_lignite), number_of_units, USER_INPUT()),
      UPDATE(L(energy_power_ultra_supercritical_lignite), preset_demand_by_electricity_production, V(energy_power_ultra_supercritical_lignite, production_based_on_number_of_units))
    )
- priority = 0
- max_value = 300.0
- max_value_gql = present:PRODUCT(DIVIDE(Q(total_electricity_produced),V(energy_power_ultra_supercritical_lignite,"electricity_output_capacity * 8760 * 3600")),2)
- min_value = 0.0
- start_value_gql = present:V(energy_power_ultra_supercritical_lignite,number_of_units)
- step_value = 0.1
- unit = #
- update_period = future
- dependent_on = has_lignite
