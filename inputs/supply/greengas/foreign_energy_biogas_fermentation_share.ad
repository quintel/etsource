- query =
    EACH(
        UPDATE(LINK(foreign_energy_distribution_greengas,foreign_energy_biogas_fermentation), share, DIVIDE(USER_INPUT(),100))
    )
- share_group = foreign_greengas
- priority = 0
- max_value = 100.0
- min_value = 0.0
- start_value_gql = DIVIDE(present:V(foreign_energy_biogas_fermentation, demand), present:V(foreign_energy_distribution_greengas, demand)) * 100.0
- step_value = 0.1
- unit = %
- update_period = future
