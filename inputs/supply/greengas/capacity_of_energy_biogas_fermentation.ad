- query =
    EACH(
        UPDATE(
            V(energy_biogas_fermentation),
            number_of_units,
            USER_INPUT() / V(energy_biogas_fermentation, typical_input_capacity)
        ),
        UPDATE(
            L(energy_biogas_fermentation),
            preset_demand_by_greengas_production,
            V(energy_biogas_fermentation, greengas_production_based_on_number_of_units)
        )
    )
- priority = 0
- max_value_gql = present:MAX(500,DIVIDE(Q(total_gas_consumed),PRODUCT(V(energy_biogas_fermentation,full_load_hours),MJ_PER_MWH)))
- min_value = 0.0
- start_value_gql = present:PRODUCT(V(energy_biogas_fermentation,number_of_units),V(energy_biogas_fermentation,typical_input_capacity))
- step_value = 0.1
- unit = MW
- update_period = future
