- query =
    EACH(
        UPDATE(
            V(energy_greengas_dry_biomass_gasification),
            number_of_units,
            USER_INPUT() / V(energy_greengas_dry_biomass_gasification, typical_input_capacity)
        ),
        UPDATE(
            L(energy_greengas_dry_biomass_gasification),
            preset_demand_by_greengas_production,
            V(energy_greengas_dry_biomass_gasification, greengas_production_based_on_number_of_units)
        )
    )
- priority = 0
- max_value_gql = present:MAX(500,DIVIDE(Q(total_gas_consumed),PRODUCT(V(energy_greengas_dry_biomass_gasification,full_load_hours),MJ_PER_MWH)))
- min_value = 0.0
- start_value_gql = present:PRODUCT(V(energy_greengas_dry_biomass_gasification,number_of_units),V(energy_greengas_dry_biomass_gasification,typical_input_capacity))
- step_value = 0.1
- unit = MW
- update_period = future
