# Changing efficiency should update electricity output conversion
# Number of units should remain unchanged as total output capacity should remain unchanged
# Priority set to 1 to set prior to input for capacity

- query =
    original_nou = V(energy_power_ultra_supercritical_network_gas, number_of_units);

    EACH(
      UPDATE(OUTPUT_SLOTS(V(energy_power_ultra_supercritical_network_gas), electricity), conversion, DIVIDE(USER_INPUT(), 100.0)),
      UPDATE(V(energy_power_ultra_supercritical_network_gas), number_of_units, original_nou),
      UPDATE(V(energy_power_ultra_supercritical_network_gas), preset_demand_by_electricity_production, V(energy_power_ultra_supercritical_network_gas, production_based_on_number_of_units))
    )
- priority = 1
- max_value_gql = present:(V(energy_power_ultra_supercritical_network_gas, electricity_output_conversion) * 100.0) + 10.0
- min_value_gql = present:(V(energy_power_ultra_supercritical_network_gas, electricity_output_conversion) * 100.0) - 10.0
- start_value_gql = present:V(energy_power_ultra_supercritical_network_gas, electricity_output_conversion) * 100.0
- step_value = 1.0
- unit = %
- update_period = future
