- query =
  EACH(
    UPDATE(MEDGE(industry_chemicals_fertilizers_captured_processes_co2, industry_chemicals_fertilizers_capture_potential_processes_co2), share, DIVIDE(USER_INPUT(),100.0)),
    UPDATE(MEDGE(industry_chemicals_fertilizers_emitted_co2, industry_chemicals_fertilizers_capture_potential_processes_co2), share, 1.0 - DIVIDE(USER_INPUT(),100.0)),
    UPDATE(
      V(industry_chemicals_fertilizers_steam_methane_reformer_hydrogen), 
      free_co2_factor, 
      PRODUCT(
        DIVIDE(USER_INPUT(),100.0),
        1.0
      )
    )
  )
- priority = 0
- max_value = 100.0
- min_value = 0.0
- start_value_gql = present:V(MEDGE(industry_chemicals_fertilizers_capture_potential_processes_co2, industry_chemicals_fertilizers_captured_processes_co2), parent_share) * 100.0
- step_value = 1.0
- unit = %
- update_period = future
- disabled_by = external_coupling_industry_chemical_fertilizers_processes_ccus_captured_co2