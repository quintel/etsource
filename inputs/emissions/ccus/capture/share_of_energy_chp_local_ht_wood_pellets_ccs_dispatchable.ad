

- query =
    wood_pellets_ht_without_ccs_share = PRODUCT(DIVIDE(INPUT_VALUE(share_of_energy_chp_local_ht_wood_pellets),100.0),(1.0 - DIVIDE(USER_INPUT(),100.0)));
    wood_pellets_ht_with_ccs_share = PRODUCT(DIVIDE(INPUT_VALUE(share_of_energy_chp_local_ht_wood_pellets),100.0),DIVIDE(USER_INPUT(),100.0));

    EACH(
        UPDATE(
            V(energy_chp_local_ht_wood_pellets_dispatchable),
            number_of_units,
            wood_pellets_ht_without_ccs_share * INPUT_VALUE(capacity_of_energy_chp_local_wood_pellets_dispatchable) / V(energy_chp_local_ht_wood_pellets_dispatchable, electricity_output_capacity)
        ),
        UPDATE(
            L(energy_chp_local_ht_wood_pellets_dispatchable),
            preset_demand_by_electricity_production,
            V(energy_chp_local_ht_wood_pellets_dispatchable, production_based_on_number_of_units)
        ),
        UPDATE(
            V(energy_chp_local_ht_wood_pellets_ccs_dispatchable),
            number_of_units,
            wood_pellets_ht_with_ccs_share * INPUT_VALUE(capacity_of_energy_chp_local_wood_pellets_dispatchable) / V(energy_chp_local_ht_wood_pellets_ccs_dispatchable, electricity_output_capacity)
        ),
        UPDATE(
            L(energy_chp_local_ht_wood_pellets_ccs_dispatchable),
            preset_demand_by_electricity_production,
            V(energy_chp_local_ht_wood_pellets_ccs_dispatchable, production_based_on_number_of_units)
        ),
    )


- priority = 1
- max_value = 100.0
- min_value = 0.0
- start_value_gql = 
    present:DIVIDE(
          V(energy_chp_local_ht_wood_pellets_ccs_dispatchable,"number_of_units * electricity_output_capacity"),
          SUM(
            V(energy_chp_local_ht_wood_pellets_must_run,"number_of_units * electricity_output_capacity"),
            V(energy_chp_local_ht_wood_pellets_ccs_dispatchable,"number_of_units * electricity_output_capacity")
          )
        )
- step_value = 1.0
- unit = %
- update_period = future
