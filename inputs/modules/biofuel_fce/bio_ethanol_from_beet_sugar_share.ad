- query = 
    EACH(
        UPDATE_FCE(CARRIER(bio_ethanol),sugar_beets, USER_INPUT() / 100),
        UPDATE(LINK(energy_production_bio_ethanol,energy_production_bio_ethanol_from_sugar_beets), share, DIVIDE(USER_INPUT(),100))
    )
- share_group = bio_ethanol_fce
- priority = 0
- max_value = 100.0
- min_value = 0.0
- start_value_gql = present: FCE_START_VALUE(bio_ethanol, sugar_beets) * 100
- step_value = 0.1
- unit = %
- update_period = future
