        UPDATE(V(energy_interconnector_6_imported_electricity, merit_order), type, producer);
        UPDATE(V(energy_interconnector_6_imported_electricity, merit_order), subtype, import_must_run);
        UPDATE(V(energy_interconnector_6_imported_electricity, merit_order), group, interconnector_6_import_must_run);
