- query =
    EACH(
        UPDATE(
            V(energy_chp_supercritical_waste_mix),
            number_of_units,
            (1.0 - DIVIDE(USER_INPUT(),100.0)) * INPUT_VALUE(external_coupling_energy_chp_supercritical_waste_mix_capacity) / V(energy_chp_supercritical_waste_mix, electricity_output_capacity)
        ),
        UPDATE(
            L(energy_chp_supercritical_waste_mix),
            preset_demand_by_electricity_production,
            V(energy_chp_supercritical_waste_mix, production_based_on_number_of_units)
        ),
        UPDATE(
            V(energy_chp_supercritical_ccs_waste_mix),
            number_of_units,
            DIVIDE(USER_INPUT(),100.0) * INPUT_VALUE(external_coupling_energy_chp_supercritical_waste_mix_capacity) / V(energy_chp_supercritical_ccs_waste_mix, electricity_output_capacity)
        ),
        UPDATE(
            L(energy_chp_supercritical_ccs_waste_mix),
            preset_demand_by_electricity_production,
            V(energy_chp_supercritical_ccs_waste_mix, production_based_on_number_of_units)
        )
    )
- priority = 1
- max_value = 100.0
- min_value = 0.0
- start_value_gql = present:DIVIDE(V(energy_chp_supercritical_ccs_waste_mix,"number_of_units*electricity_output_capacity"),SUM(V(energy_chp_supercritical_ccs_waste_mix,"number_of_units*electricity_output_capacity"),V(energy_chp_supercritical_waste_mix, "number_of_units*electricity_output_capacity")))
- step_value = 1.0
- unit = %
- update_period = future
- coupling_groups = [ctm]
