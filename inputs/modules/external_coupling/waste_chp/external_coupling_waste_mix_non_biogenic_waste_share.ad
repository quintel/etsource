- query = 
    UPDATE(
      EDGE(energy_distribution_non_biogenic_waste, energy_distribution_waste_mix),
      share,
      DIVIDE(USER_INPUT(), 100.0)
    )
- share_group = waste_mix
- priority = 100
- max_value = 100.0
- min_value = 0.0
- start_value_gql = present:V(energy_distribution_non_biogenic_waste,share_of_energy_distribution_waste_mix) * 100
- step_value = 0.1
- unit = %
- update_period = future
- coupling_groups = [ctm]
