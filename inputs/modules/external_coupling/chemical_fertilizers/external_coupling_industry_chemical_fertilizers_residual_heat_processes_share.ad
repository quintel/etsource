- query =
    UPDATE(
      EDGE(
        industry_chemicals_fertilizers_production,
        industry_chemicals_fertilizers_processes_potential_residual_heat
      ),
      share,
      DIVIDE(USER_INPUT(),100)
    )
- priority = 0
- max_value = 100.0
- min_value = 0.0
- start_value_gql = present:V(industry_chemicals_fertilizers_production,share_of_industry_chemicals_fertilizers_processes_potential_residual_heat) * 100
- step_value = 0.1
- unit = %
- update_period = future
