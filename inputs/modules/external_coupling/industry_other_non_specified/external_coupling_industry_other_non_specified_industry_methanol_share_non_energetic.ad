- share_group = external_coupling_other_industry_non_energetic
- priority = 0
- max_value = 100.0
- min_value = 0.0
- start_value_gql = present:Q(share_of_methanol_in_other_non_specified_industry_non_energetic) * 100.0
- step_value = 0.1
- unit = %
- update_period = future
- coupling_groups = [ctm, industry_other_non_specified]
