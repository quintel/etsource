      EDGE(industry_other_paper_heater_electricity, industry_useful_demand_for_other_paper_useable_heat),
      share,
      DIVIDE(USER_INPUT(), 100)
    )
- share_group = other_paper_heat
- priority = 0
- max_value = 100.0
- min_value = 0.0
- start_value_gql = present:V(industry_other_paper_heater_electricity,share_of_industry_useful_demand_for_other_paper_useable_heat) * 100
- step_value = 0.1
- unit = %
- update_period = future
- disabled_by_couplings = [industry_other_paper]
