- query =
    total_demand_other_industry_energetic = Q(final_demand_of_aggregated_other_industry_energetic);
    EACH(
      UPDATE(EXCLUDE(FILTER(Q(aggregated_other_industry_energetic_converters), "electricity_input_conversion > 0.0"),V(industry_useful_demand_for_other_non_specified_electricity)), preset_demand, 0),
      UPDATE(V(industry_useful_demand_for_other_non_specified_electricity), preset_demand, USER_INPUT() * total_demand_other_industry_energetic * 10000000)
    )
- share_group = other_industry_energetic
- priority = 0
- max_value = 100.0
- min_value = 0.0
- start_value_gql = present:Q(share_of_electricity_in_aggregated_other_industry) * 100
- step_value = 0.1
- unit = %
- update_period = future
