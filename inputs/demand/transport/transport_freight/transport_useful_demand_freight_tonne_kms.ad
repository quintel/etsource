- query =
      EACH(
            UPDATE(V(transport_useful_demand_freight_tonne_kms), preset_demand, USER_INPUT()),
            UPDATE(V(transport_useful_demand_trucks), number_of_units, USER_INPUT()),
            UPDATE(V(transport_useful_demand_vans), number_of_units, USER_INPUT())
            )
- priority = 1
- max_value = 5.0
- min_value = -5.0
- start_value = 0.0
- step_value = 0.1
- unit = %
- update_period = future
- update_type = %y
