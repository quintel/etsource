- query =
    UPDATE(
      V(OUTPUT_SLOTS(LOOKUP(buildings_chp_engine_biogas),steam_hot_water), "links.detect{|l| !l.flexible? }"),
      share, USER_INPUT() / 100.0
    )
- share_group = district_heating_buildings
- priority = 0
- max_value = 100.0
- min_value = 0.0
- start_value_gql = present:V(buildings_chp_engine_biogas,share_of_buildings_final_demand_steam_hot_water) * 100
- step_value = 0.1
- unit = %
- update_period = future
