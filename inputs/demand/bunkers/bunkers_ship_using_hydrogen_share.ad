- query =
    UPDATE(
      EDGE(bunkers_ship_using_hydrogen, bunkers_useful_demand_ships),
      share,
      DIVIDE(USER_INPUT(), 100)
    )
- share_group = bunkers_shipping
- priority = 0
- max_value = 100.0
- min_value = 0.0
- start_value_gql = present:V(bunkers_ship_using_hydrogen,share_of_bunkers_useful_demand_ships) * 100
- step_value = 0.1
- unit = %
- update_period = future
