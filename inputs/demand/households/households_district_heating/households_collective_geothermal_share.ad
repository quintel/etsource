- query =
    UPDATE(
      V(OUTPUT_SLOTS(LOOKUP(households_collective_geothermal),steam_hot_water), "links.detect{|l| !l.flexible? }"),
      share, USER_INPUT() / 100.0
    )
- share_group = district_heating_households
- priority = 10
- max_value = 100.0
- min_value = 0.0
- start_value_gql = present:V(households_collective_geothermal,share_of_households_final_demand_steam_hot_water) * 100
- step_value = 0.1
- unit = %
- update_period = future
