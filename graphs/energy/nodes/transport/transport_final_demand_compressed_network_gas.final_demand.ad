- use = energetic
- energy_balance_group = final consumption
- groups = [final_demand_group, co2_emissions_primary]
- graph_methods = [demand]
- free_co2_factor = 0.0

~ demand = EB(road, natural_gas) * SHARE(transport_final_demand_for_road_natural_gas_parent_share, transport_final_demand_for_road_compressed_network_gas) + EB(road, biogases) * SHARE(transport_final_demand_for_road_greengas_parent_share, transport_final_demand_for_road_compressed_network_gas)
