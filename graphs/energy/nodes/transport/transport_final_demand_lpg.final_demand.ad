- use = energetic
- energy_balance_group = final consumption
- groups = [final_demand_group, co2_emissions_refinery_products]
- graph_methods = [demand]
- free_co2_factor = 0.0

~ demand =
    EB("transport", "liquefied_petroleum_gases_(LPG)") -
    EB("non-specified (transport)", "liquefied_petroleum_gases_(LPG)") -
    EB("pipeline transport", "liquefied_petroleum_gases_(LPG)")
