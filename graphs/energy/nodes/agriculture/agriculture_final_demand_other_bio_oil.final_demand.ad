- use = energetic
- energy_balance_group = final consumption
- groups = [co2_emissions_primary]
- graph_methods = [demand]
- free_co2_factor = 0.0

~ demand =
    EB("agriculture/forestry", "non-specified primary biofuels and waste") +
    EB("agriculture/forestry", biogasoline) +
    EB("agriculture/forestry", other_liquid_biofuels)
