- use = energetic
- energy_balance_group = final consumption
- groups = [final_demand_group, co2_emissions_primary]
- graph_methods = [input]
- free_co2_factor = 0.0

~ input.diesel = EFFICIENCY(agriculture_final_demand_crude_oil, input, diesel)

~ input.biodiesel = EFFICIENCY(agriculture_final_demand_crude_oil, input, biodiesel)

~ input.kerosene = EFFICIENCY(agriculture_final_demand_crude_oil, input, kerosene)

~ input.bio_kerosene = EFFICIENCY(agriculture_final_demand_crude_oil, input, bio_kerosene)

~ input.lpg = EFFICIENCY(agriculture_final_demand_crude_oil, input, lpg)

~ input.bio_oil = EFFICIENCY(agriculture_final_demand_crude_oil, input, bio_oil)

~ input.crude_oil = EFFICIENCY(agriculture_final_demand_crude_oil, input, crude_oil)
