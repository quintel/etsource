- use = energetic
- energy_balance_group = final consumption
- groups = [final_demand_group, co2_emissions_primary]
- graph_methods = [demand]
- free_co2_factor = 0.0

~ demand =
    EB(commercial_and_public_services, "crude/ngl/feedstocks_(if_no_detail)") +
    EB(commercial_and_public_services, crude_oil) +
    EB(commercial_and_public_services, natural_gas_liquids) +
    EB(commercial_and_public_services, "additives/blending_components") +
    EB(commercial_and_public_services, other_hydrocarbons) +
    EB(commercial_and_public_services, refinery_gas) +
    EB(commercial_and_public_services, ethane) +
    EB(commercial_and_public_services, "liquefied_petroleum_gases_(lpg)") +
    EB(commercial_and_public_services, motor_gasoline) +
    EB(commercial_and_public_services, aviation_gasoline) +
    EB(commercial_and_public_services, gasoline_type_jet_fuel) +
    EB(commercial_and_public_services, kerosene_type_jet_fuel) +
    EB(commercial_and_public_services, other_kerosene) +
    EB(commercial_and_public_services, "gas/diesel_oil") +
    EB(commercial_and_public_services, fuel_oil) +
    EB(commercial_and_public_services, naphtha) +
    EB(commercial_and_public_services, "white_spirit_&_sbp") +
    EB(commercial_and_public_services, lubricants) +
    EB(commercial_and_public_services, bitumen) +
    EB(commercial_and_public_services, paraffin_waxes) +
    EB(commercial_and_public_services, petroleum_coke) +
    EB(commercial_and_public_services, "non-specified_oil_products")
