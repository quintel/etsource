- use = energetic
- groups = [final_demand_group, co2_emissions_primary, final_demand_electricity]
- graph_methods = [demand]
- availability = 0.0
- free_co2_factor = 0.0

~ demand =
    EB(industry, electricity) -
    EB(energy_industry_own_use, electricity) +
    EB("own_use_in_electricity,_CHP_and_heat_plants", electricity)
