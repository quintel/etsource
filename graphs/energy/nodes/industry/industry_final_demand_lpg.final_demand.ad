- use = energetic
- energy_balance_group = final consumption
- groups = []
- graph_methods = [demand]
- free_co2_factor = 0.0

~ demand =
    EB(industry, "liquefied_petroleum_gases_(lpg)") -
    EB(energy_industry_own_use, "liquefied_petroleum_gases_(lpg)") +
    EB("own_use_in_electricity,_CHP_and_heat_plants", "liquefied_petroleum_gases_(lpg)")
