- use = energetic
- energy_balance_group = final consumption
- groups = []
- graph_methods = [demand]
- free_co2_factor = 0.0

~ demand =
    EB(industry, "gas/diesel_oil") -
    EB(energy_industry_own_use, "gas/diesel_oil") +
    EB("own_use_in_electricity,_CHP_and_heat_plants", "gas/diesel_oil") +
    EB(blast_furnaces_own_use, "gas/diesel_oil") +
    EB(coke_ovens_own_use, "gas/diesel_oil")
